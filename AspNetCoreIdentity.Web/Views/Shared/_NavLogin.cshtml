@if (User.Identity!.IsAuthenticated)
{
    <ul class="navbar-nav me-0">
        <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                @User.Identity.Name
            </a>
            <ul class="dropdown-menu">
                <li><a class="dropdown-item" asp-route-returnurl="/Home/Index" asp-controller="Member" asp-action="LogOut">Çıkış Yap</a></li>
                <li><a class="dropdown-item" asp-route-returnurl="/Home/Privacy" asp-controller="Member" asp-action="LogOut">Çıkış Yap 2</a></li>
            </ul>
        </li>
                           
    </ul>
}
else
{
    <a class="btn btn-outline-warning mx-1"asp-area="" asp-controller="Home" asp-action="SignUp">Üye ol</a>
    <a class="btn btn-outline-success" asp-area="" asp-controller="Home" asp-action="SignIn">Giriş Yap</a>
}